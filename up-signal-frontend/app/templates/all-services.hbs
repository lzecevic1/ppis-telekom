<div class="list-top">
  <p>PREGLED SVIH USLUGA</p>
</div>
<div class="col-md-10 suppliers-body">
{{#cp-panels accordion=true as |panels|}}
  {{#each model as |service|}}
  <div class="panel panel-primary">
  {{#panels.panel as |panel|}}
    {{#panel.toggle}}
      <div class="panel-heading">{{service.id}}</div>
    {{/panel.toggle}}
    {{#panel.body}}
      <div class="panel-body">
        <img class='service-row-image' src="/assets/images/technology.png"><br>
        <p>Tip usluge: {{service.type}}</p>
        <p>Opis: {{service.description}}</p>
        <p>Cijena: {{service.price}}</p>
        <p>Odgovorna osoba: {{service.responsiblePerson}}</p>
        <button class="btn btn-primary list-item-btn">Izmjena</button>
        <button class="btn btn-primary list-item-btn">Brisanje</button><br>
      </div>
    {{/panel.body}}
  {{/panels.panel}}
  </div>
  {{/each}}
{{/cp-panels}}
</div>